all: clean benchmark

benchmark: acak.o objective.o fpa.o benchmark.o
	g++ acak.o objective.o fpa.o benchmark.o -o benchmark

benchmark.o: benchmark.cpp
	g++ -std=c++11 -c benchmark.cpp

fpa.o: fpa.cpp
	g++ -std=c++11 -c fpa.cpp

objective.o: objective.cpp
	g++ -c objective.cpp

acak.o: acak.cpp
	g++ -std=c++11 -c acak.cpp

clean:
	rm -rf *o benchmark